{
  "expo": {
    "name": "expo-indoor-positioning",
    "slug": "expo-indoor-positioning",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.irvansn.expoindoorpositioning"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.irvansn.expoindoorpositioning"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2ff37d19-b19c-4ad6-a797-07927c1a8219"
      }
    },
    "plugins": [
      [
        "react-native-wifi-reborn",
        {
          "fineLocationPermission": true
        }
      ]
    ],
    "owner": "irvansn"
  }
}